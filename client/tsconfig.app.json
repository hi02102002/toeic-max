{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "type.d.ts",
        "./typed-router.d.ts",
        "./shims.d.ts"
    ],
    "exclude": [
        "src/**/__tests__/*"
    ],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "moduleResolution": "Bundler",
        "types": [
            "vite-plugin-vue-layouts/client",
            "unplugin-vue-router/client",
        ]
    },
}